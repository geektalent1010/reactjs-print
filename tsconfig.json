{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "removeComments": true,
    "strict": true,
    "target": "es5"
  },
  "include": [
    "./src"
  ]
}
